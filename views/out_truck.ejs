<!DOCTYPE html>
<html lang="et">
<head>
  <meta charset="utf-8">
  <title>Mattias Tamm</title>
</head>
<body>
  <img src="/banner/vp_banner_2024.png" alt="veebiprogrammeerimise 2024. aasta kursuse bänner">
  <h1>Mattias Tamm</h1>
  <hr>
  <ul>
    <li><a href="/">Avalehele</a></li>
    <li><a href="/viljavedu">Viljavedu</a></li>
  </ul>
  <hr>
  <h2>Lisa auto mass lahkumisel</h2>
  <form method="POST">
    <label for="truck">Auto registrinumber</label>
    <br>
    <select id="truck" name="truck">
      <option selected disabled>Vali väljumata auto number</option>
      <% deliveryList.forEach(function(delivery) { %>
        <option value="<%= delivery.id %>"><%= delivery.truck %></option>
      <% }); %>
    </select>
    <br>
    <label for="massOut">Sisesta auto mass väljumisel(kilogrammides): </label>
    <input type="number" id="massOut" name="massOut" placeholder="Mass ...">
    <br>
    <input type="submit" id="exitSubmit" name="exitSubmit" value="Salvesta">
  </form>
  <p><%= locals.notice || "" %></p>
</body>
</html>